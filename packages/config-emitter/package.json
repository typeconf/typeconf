{
  "name": "@typeconf/config-emitter",
  "version": "0.2.2",
  "description": "Typeconf internal config emitter",
  "homepage": "https://typeconf.dev",
  "readme": "https://github.com/typeconf/typeconf/blob/main/README.md",
  "author": "Ivan Chebykin",
  "license": "Apache-2.0",
  "keywords": [
    "typeconf",
    "config-emitter"
  ],
  "type": "module",
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typeconf/typeconf.git",
    "directory": "packages/config-emitter"
  },
  "dependencies": {
    "@typespec-tools/emitter-typescript": "^0.3.0",
    "@typespec/compiler": "^0.59.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "build": "tsc && cp package-dist.json dist/package.json",
    "watch": "tsc --watch",
    "test": "node --test",
    "format": "prettier .  --write",
    "format:check": "prettier --check ."
  },
  "publishConfig": {
    "access": "public"
  }
}
