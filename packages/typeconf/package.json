{
  "name": "@typeconf/typeconf",
  "version": "0.3.0",
  "description": "Typeconf CLI for defining configs with types.",
  "homepage": "https://typeconf.dev",
  "readme": "https://github.com/typeconf/typeconf/blob/main/README.md",
  "author": "Ivan Chebykin",
  "license": "Apache-2.0",
  "keywords": [
    "typeconf",
    "cli"
  ],
  "main": "dist/src/index.js",
  "type": "module",
  "bin": {
    "typeconf": "dist/src/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typeconf/typeconf.git",
    "directory": "packages/typeconf"
  },
  "scripts": {
    "build": "npx tsc",
    "test:simple": "../../tests/compile-examples.sh simple",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "npm run build && node dist/src/cli.js"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.0",
    "@types/prompts": "^2.4.9",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.46.1",
    "@typeconf/config-emitter": "workspace:^",
    "@typeconf/sdk": "workspace:^",
    "@types/cli-table": "^0.3.4",
    "@typespec/compiler": "^0.66.0",
    "ajv": "^8.17.1",
    "cli-table": "^0.3.11",
    "commander": "^12.1.0",
    "esbuild": "^0.25.1",
    "glob": "^11.0.1",
    "jsonc": "^2.0.0",
    "prompts": "^2.4.2",
    "ts-morph": "^25.0.1",
    "ts-to-zod": "^3.15.0",
    "typescript": "^5.7.2",
    "uuid": "^11.1.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
