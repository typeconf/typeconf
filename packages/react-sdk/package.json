{
  "name": "@typeconf/react-sdk",
  "version": "0.3.0",
  "description": "Typeconf SDK for React applications",
  "homepage": "https://typeconf.dev",
  "readme": "https://github.com/typeconf/typeconf/blob/main/README.md",
  "author": "Ivan Chebykin",
  "license": "Apache-2.0",
  "keywords": [
    "typeconf",
    "react",
    "sdk"
  ],
  "type": "module",
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./server": {
      "types": "./dist/src/server.d.ts",
      "default": "./dist/src/server.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typeconf/typeconf.git",
    "directory": "packages/react-sdk"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@typeconf/sdk": "workspace:^",
    "@types/lodash": "^4.17.15",
    "lodash": "^4.17.21",
    "react": ">=18"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@types/react": ">=18",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
