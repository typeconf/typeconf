{
  "name": "@typeconf/sdk",
  "version": "0.3.0-prerelease2",
  "description": "Typeconf SDK for reading typed configs in your repository",
  "homepage": "https://typeconf.dev",
  "readme": "https://github.com/typeconf/typeconf/blob/main/README.md",
  "author": "Ivan Chebykin",
  "license": "Apache-2.0",
  "keywords": [
    "typeconf",
    "node",
    "config",
    "sdk"
  ],
  "type": "module",
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typeconf/typeconf.git",
    "directory": "packages/sdk"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "vitest",
    "start": "npm run build && node dist/src/index.js"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "dotenv": "^16.4.7",
    "typescript": "^5.7.2",
    "vitest": "^3.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.46.1",
    "cli-table": "^0.3.11",
    "jiti": "^2.4.0",
    "jsonc": "^2.0.0",
    "prompts": "^2.4.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  }
}
